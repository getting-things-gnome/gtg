<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="calendar">
    <property name="events">GDK_STRUCTURE_MASK | GDK_PROXIMITY_OUT_MASK</property>
    <property name="resizable">False</property>
    <property name="modal">True</property>
    <property name="destroy_with_parent">True</property>
    <child>
      <placeholder/>
    </child>
    <property name="child">
      <object class="GtkBox" id="vbox5">
        <property name="margin_start">12</property>
        <property name="margin_end">12</property>
        <property name="margin_top">12</property>
        <property name="margin_bottom">12</property>
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkCalendar" id="calendar1"/>
        </child>
        <child>
          <object class="GtkBox" id="fuzzydate_btns">
            <property name="margin_top">8</property>
            <property name="homogeneous">True</property>
            <child>
              <object class="GtkButton" id="button_now">
                <property name="label" translatable="yes">Now</property>
                <property name="receives_default">True</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_soon">
                <property name="label" translatable="yes">Soon</property>
                <property name="receives_default">True</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_someday">
                <property name="label" translatable="yes">Someday</property>
                <property name="receives_default">True</property>
              </object>
            </child>
            <style>
              <class name="linked"/>
            </style>
          </object>
        </child>
        <child>
          <object class="GtkButton" id="button_clear">
            <property name="label" translatable="yes">Clear</property>
            <property name="receives_default">True</property>
            <property name="margin_top">8</property>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
